; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[{
"base": "ScrollContainer",
"class": &"OpenXRActionSetsEditor",
"language": &"GDScript",
"path": "res://addons/godot-openxr/editor/OpenXRActionSetsEditor.gd"
}, {
"base": "MarginContainer",
"class": &"OpenXRInteractionProfileBase",
"language": &"GDScript",
"path": "res://addons/godot-openxr/editor/interaction_profiles/OpenXRInteractionProfileBase.gd"
}]
_global_script_class_icons={
"OpenXRActionSetsEditor": "",
"OpenXRInteractionProfileBase": ""
}

[application]

config/name="Godot OpenXR demo"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[display]

window/vsync/use_vsync=false

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-openxr/plugin.cfg")

[gdnative]

singletons=[]

[input]

Fire={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"store_command":true,"alt_pressed":false,"shift_pressed":false,"meta_pressed":false,"command_pressed":false,"pressed":false,"keycode":0,"physical_keycode":0,"unicode":0,"echo":false,"script":null)
, Object(InputEventJoypadButton,"resource_local_to_scene":false,"resource_name":"","device":0,"button_index":0,"pressure":0.0,"pressed":false,"script":null)
]
}

[layer_names]

3d_render/layer_20="Shadow only"
3d_physics/layer_1="Default"
3d_physics/layer_11="UI"
3d_physics/layer_20="Player"

[rendering]

vulkan/rendering/back_end=1
xr/enabled=true
quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
quality/filters/msaa=1
